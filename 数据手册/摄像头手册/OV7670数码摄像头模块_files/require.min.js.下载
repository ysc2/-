var version = '20231120'
var com_import_list = [
    '<script type="text/javascript" src="/static/js/browser-polyfill.min.js"></script>',
    '<script type="text/javascript" src="/static/js/vue-2.5.16.js"></script>',
    '<script type="text/javascript" src="/static/js/axios-0.18.0.min.js"></script>',
    '<script type="text/javascript" src="/static/js/jquery.min.js"></script>',
    '<script type="text/javascript" src="/static/js/jquery.lazyload.min.js"></script>',
    '<script type="text/javascript" src="/static/js/jquery.md5.min.js"></script>',
    '<script type="text/javascript" src="/static/js/jquery.form.min.js"></script>',
    '<script type="text/javascript" src="/static/js/base64.min.js"></script>',
];

com_import_list.push(`
<script type="text/javascript">
    $.ajax({
        url : '/api/v1/version',
        cache : false,
        async : false,
        type : "get",
        success : function (result){
            version = result.version || version;
            console.log('--------', version)
        }
    });
    console.log('==========', version)
</script>
`)
console.log(version)
document.write(com_import_list.join(''));

import_list = [
    '<link rel="stylesheet" type="text/css" href="/static/css/reset.css?v=' + version + '">',
    '<link rel="stylesheet" type="text/css" href="/static/css/main.css?v=' + version + '">',
    '<link rel="stylesheet" type="text/css" href="/static/css/screen-1024.css?v=' + version + '">',
