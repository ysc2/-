    '<script type="text/javascript" src="/static/js/function.js?v=' + version + '"></script>',
    '<script type="text/javascript" src="/static/js/common.js?v=' + version + '"></script>',
    '<script type="text/javascript" src="/static/js/module_app.js?v=' + version + '"></script>'
];

var jsObj = document.getElementById('require');
if (jsObj && jsObj.getAttribute('data') == 'goods') {
    import_list = import_list.concat([
        '<link rel="stylesheet" type="text/css" href="/static/css/cloudzoom.css?v=' + version + '">',
        '<link rel="stylesheet" type="text/css" href="/static/css/lightbox.min.css?v=' + version + '">',
