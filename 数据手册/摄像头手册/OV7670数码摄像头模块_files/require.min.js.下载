        '<script type="text/javascript" src="/static/js/lightbox.min.js?v=' + version + '"></script>',
        '<script type="text/javascript" src="/static/js/video.min.js?v=' + version + '"></script>',
	    '<script type="text/javascript" src="/static/js/goods_slide.js?v=' + version + '"></script>',
    ])
}

document.write(import_list.join(''));
